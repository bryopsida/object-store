{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testTimeout": 240000,
  "verbose": true,
  "moduleNameMapper": {
    "app.js": "app",
    "./auth/authPlugin.js": "./auth/authPlugin",
    "./routes/index.js": "./routes/index",
    "./services/storageAreaService.js": "./services/storageAreaService",
    "./services/objectStorageService.js": "./services/objectStorageService",
    "./services/userService.js": "./services/userService",
    "./objectController.js": "./objectController",
    "./storageStatusController.js": "./storageStatusController"
  },
  "extensionsToTreatAsEsm": [".ts"],
  "transform": {
    "\\.[jt]sx?$": [
      "ts-jest",
      {
        "useESM": true
      }
    ]
  }
}
